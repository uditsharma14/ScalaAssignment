package com.scala.udit

class TemperaroryEmployee(val Id: Int, val name: String,
                      val wagePerDay: Float) {
  var empId: Int = Id
  var empName: String = name
  var perDayWage: Float = wagePerDay

  def showDetails {
    print("Employee Id :" + empId);
    print("Employee Name :" + empName);
    print("Employee per Day Wages :" + perDayWage);
  }

}