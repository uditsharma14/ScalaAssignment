package com.scala.udit

class RegularEmployee(val Id: Int, val name: String, 
    val salary: Float) {
  var empId: Int = Id
  var empName: String = name
  var wagePerDay

  def showDetails(empId: Int, empName: String) {
    print("Employee Id :" + empId);
    print("Employee Name :" + empName);
  }

}